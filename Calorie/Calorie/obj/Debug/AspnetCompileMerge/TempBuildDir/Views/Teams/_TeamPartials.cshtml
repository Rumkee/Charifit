
@model Calorie.Models.Teams.TeamSearchResultsVM

@foreach (var T in @Model.TeamResults)
{
        @Html.Partial("..\\Teams\\_TeamPartial", new Calorie.Models.Teams.TeamVM() { ShowHyperLinks = true, ShowSelector = @Model.ShowSelector, Team = T })            
}


@if (!Model.TeamResults.Any())
{

    <div class="alert alert-warning fade in " role="alert">
        <div class="row">
            <div class="col-xs-12 col-sm-9">
                <i class="fa fa-frown-o" aria-hidden="true"></i>&nbsp;<span>Oooops! it looks like we couldn't find anything matching your search.</span>
            </div>
        </div>
    </div>

    
}

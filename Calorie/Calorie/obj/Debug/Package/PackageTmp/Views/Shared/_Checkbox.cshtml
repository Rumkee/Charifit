@model string


@{    
    var thisTick = "thisTick" + Guid.NewGuid().ToString().Replace("-", "").Substring(0, 5);    
}

<div class="tickbox" >
    <span id="@thisTick" class="tickbox-tick calorie-green-color glyphicon glyphicon-ok"></span>
</div>

<script>
    new calorie.helpers.checkboxController($('#@Model'),$('#@thisTick'));   
</script>

